---
import "../styles/global.css";
import Header from "../components/header/header.jsx";

const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <slot name="head"></slot>
  <title>{title}</title>
</head>
<body>
  <Header title="SolidAstro"></Header>
  <slot></slot>
</body>
</html>